<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Sherlock.__Theme" parent="@android:style/Theme.Holo">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Sherlock.__Theme.Light" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Theme.OrcaDialog" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowBackground">@drawable/orca_panel_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>
    <style name="Theme.Orca" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:listViewStyle">@style/Theme.Orca.ListView</item>
    </style>
    <style name="Theme.Orca.Preferences" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:listViewStyle">@style/Theme.Orca.ListView</item>
    </style>
    <style name="Theme.Orca.ClickableRow" parent="@android:style/Theme.Holo.Light">
        <item name="android:background">@drawable/orca_item_background_holo_light</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="Theme.FacebookDark" parent="@android:style/Theme.Holo">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Theme.Facebook" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="Theme.Facebook.Preferences" parent="@android:style/Theme.Holo.Light" />
    <style name="Theme.FacebookDialogSimple" parent="@android:style/Theme.Holo.Light.Dialog" />
    <style name="GDPDialogTheme" parent="@android:style/Theme.Holo.Dialog" />
    <style name="GDPDialogTitle" parent="@android:style/TextAppearance.Large">
        <item name="android:visibility">gone</item>
    </style>
    <style name="GDPTitleDivider">
        <item name="android:visibility">gone</item>
    </style>
    <style name="GDPMessageText">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>
    <style name="GDPNuxText">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">#ff3cb5e2</item>
    </style>
    <style name="GDPDialogHorizontalDivider">
        <item name="android:background">?android:dividerHorizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="GDPButtonContainer" />
    <style name="GDPDialogVerticalDivider">
        <item name="android:background">?android:dividerVertical</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="GDPDialogButton">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="Theme.Orca.ListView" parent="@android:style/Widget.ListView">
        <item name="android:listSelector">@drawable/orca_list_selector_holo_light</item>
    </style>
</resources>
