<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/dash_preference_switch_thumb_off" />
    <item android:state_checked="true" android:state_pressed="false" android:drawable="@drawable/dash_preference_switch_thumb_on" />
    <item android:state_checked="true" android:state_pressed="true" android:drawable="@drawable/dash_preference_switch_thumb_pressed_on" />
    <item android:state_checked="false" android:state_pressed="false" android:drawable="@drawable/dash_preference_switch_thumb_off" />
    <item android:state_checked="false" android:state_pressed="true" android:drawable="@drawable/dash_preference_switch_thumb_pressed_off" />
    <item android:drawable="@drawable/dash_preference_switch_thumb_off" />
</selector>
