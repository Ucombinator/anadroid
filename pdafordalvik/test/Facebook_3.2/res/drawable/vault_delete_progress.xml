        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffe1e1e1" android:endColor="#ffe1e1e1" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffe1e1e1" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
