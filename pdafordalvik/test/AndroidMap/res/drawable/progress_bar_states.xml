<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ff000001" android:endColor="#ff0d1522" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0b131e" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff223344" android:endColor="#ffaa2244" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff223344" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff144281" android:endColor="#ff06101d" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0b1f3c" />
            </shape>
        </clip>
    </item>
</layer-list>
