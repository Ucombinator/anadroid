            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff3f739a" android:endColor="#ff3f739a" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff3f739a" />
            </shape>
        </clip>
