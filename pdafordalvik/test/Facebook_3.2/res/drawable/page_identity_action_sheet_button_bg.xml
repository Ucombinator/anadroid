<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffe5e5e5" android:endColor="#fff5f5f5" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#fffdfdfd" android:endColor="#ffececec" android:angle="-90.0" />
        </shape>
    </item>
</selector>
